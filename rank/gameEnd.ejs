<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Recreation Game</title>
    <link rel="stylesheet" href="/public/gameEnd.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Jua&display=swap" rel="stylesheet">

</head>
<body>
    <div class="wrap">
        <div class="title">
            <% var quiz = data[0] %>
            <%= quiz.name %>
        </div>
        <a href="/"><img src="/public/home.png" alt="홈으로" class="homeb"></a>
       
        <br>
       
            <p>문제를 다 풀었습니다!</p>
       
        <hr class="shortline">
        <br><br>
        <div class="container">
            <div class="group">
                <% if(quiz.name == "OX"){%>
                    <a href="/quiz/ox"><div><button class="category">더 풀기</button></div></a>
                <%}%>

                <% if(quiz.name == "네글자 이어말하기"){%>
                    <a href="/quiz/fourWord"><div><button class="category">더 풀기</button></div></a>
                <%}%>

                <% if(quiz.name == "넌센스"){%>
                    <a href="/quiz/nonsense"><div><button class="category">더 풀기</button></div></a>
                <%}%>
                
                <% if(quiz.name == "상식퀴즈"){%>
                    <a href="/quiz/knowledge"><div><button class="category">더 풀기</button></div></a>
                <%}%>

                <% if(quiz.name == "국기 맞히기"){%>
                    <a href="/quiz/flag"><div><button class="category">더 풀기</button></div></a>
                <%}%>

                <% if(quiz.name == "영화"){%>
                    <a href="/quiz/movie"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "드라마"){%>
                    <a href="/quiz/drama"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "댄스곡"){%>
                    <a href="/quiz/dance"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "음식"){%>
                    <a href="/quiz/food"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "행사"){%>
                    <a href="/quiz/anniversary"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "취미생활"){%>
                    <a href="/quiz/hobby"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "직업"){%>
                    <a href="/quiz/job"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "스포츠"){%>
                    <a href="/quiz/sports"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "동물"){%>
                    <a href="/quiz/animal"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "감정"){%>
                    <a href="/quiz/emotion"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                        
                <% if(quiz.name == "악기"){%>
                    <a href="/quiz/instrument"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "속담"){%>
                    <a href="/quiz/proverb"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "인물"){%>
                    <a href="/quiz/person"><div><button class="category">더 풀기</button></div></a>
                <%}%>
        
                <% if(quiz.name == "물건"){%>
                    <a href="/quiz/object"><div><button class="category">더 풀기</button></div></a>
                <%}%>

                <% if(quiz.name == "줄줄이말해요"){%>
                    <a href="/quiz/object"><div><button class="category">더 풀기</button></div></a>
                <%}%>
                <div><a href="/"><button class="category">끝내기</button></div></a>
            </div>  
        </div>
        <br><br><br>
        <div class="rank_title">
            <img src="/public/rank.png" alt="랭킹" class="rank_image">
            랭킹
        </div>
    
        <div class="container_rank">

            <div class="rank">
                <div class="text1">순위</div>
                <div class="text2">팀명</div>
                <div class="text3">점수</div>
            </div>
    
            <div class="rank">
                <div class="circle" style="background-color: gold;">1</div>
                <div class="first_team">도현팀</div>
                <div class="Score">100</div>
            </div>
            <hr class="line_Rank">
    
            <div class="rank">
                <div class="circle" style="background-color: silver;">2</div>
                <div class="second_team">블랙핑크</div>
                <div class="Score"></div>
            </div>
            <hr class="line_Rank">
    
            <div class="rank">
                <div class="circle" style="background-color: #DE844F;">3</div>
                <div class="third_team">가나다라마바사아</div>
                <div class="Score"></div>
            </div>
            <hr class="line_Rank">
    
            <div class="rank">
                <div class="circle" style="background-color: #F6F6F6;">4</div>
                <div class="fourth_team">랭킹어렵당</div>
                <div class="Score"></div>
            </div>
            <hr class="line_Rank">
    
            <div class="rank">
                <div class="circle" style="background-color: #F6F6F6;">5</div>
                <div class="fifth_team">그만늘어나</div>
                <div class="Score">23</div>
            </div>
        </div>


    </div>
</body>
</html>
